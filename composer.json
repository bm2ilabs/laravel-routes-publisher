{
    "name": "themsaid/laravel-routes-publisher",
    "description": "A command to replace deprecated Route::controller with explicit routes.",
    "keywords": ["laravel", "routes"],
    "homepage": "https://github.com/themsaid/laravel-routes-publisher",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\RoutesPublisher\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\RoutesPublisher\\RoutesPublisherServiceProvider"
            ]
        }
    }
}
